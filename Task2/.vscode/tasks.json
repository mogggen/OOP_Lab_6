{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "Generate project",
            "command": "cmake -S . -B build",
            "type": "shell",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": "none",
        },
        {
            "label": "Build project",
            "command": "cmake --build build --target SDL2Test",
            "type": "shell",
            "options": {
                "cwd": "${workspaceFolder}"
            },
            "group": {
                "kind": "build",
                "isDefault": true
            }
        }
    ]
}